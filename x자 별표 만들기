#include <stdio.h>

int main(void) 
{
  for (int i=1; i<=11; i++) //층수
    {
      for(int j=1; j <= 11; j++)
        {
          if (j == i || j == 12 - i)
          {
            printf("*");  
          }
          else
          {
            printf(" ");
          }
        }
      printf("\n");
    }
  
  return 0;
}
